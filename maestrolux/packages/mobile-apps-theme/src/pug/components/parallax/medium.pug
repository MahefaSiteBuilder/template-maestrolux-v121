.parallax-wrap
  .inner-parallax.medium
    div(data-enllax-ratio="-0.1" data-enllax-type="foreground")
      .parallax-figure
        div
          |
          |
          svg.triangle
            include ../svg/triangle.pug
          |
          |
    div(data-enllax-ratio="-0.15" data-enllax-type="foreground")
      .parallax-figure
        div
          |
          |
          svg.circle
            include ../svg/circle.pug
          |
          |
    div(data-enllax-ratio="-0.15" data-enllax-type="foreground")
      .parallax-figure
        div.square-dot
          |
          |
          img.dot-many(src=assets+"/images/mobile/dot-many.svg" alt="dot")/
          |
          |
