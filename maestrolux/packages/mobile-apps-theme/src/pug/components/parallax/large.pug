.parallax-wrap
  .inner-parallax.large
    div(data-enllax-ratio="0" data-enllax-type="foreground")
      .parallax-figure
        div
          |
          |
          svg.triangle
            include ../svg/triangle.pug
          |
          |
    div(data-enllax-ratio="0" data-enllax-type="foreground")
      .parallax-figure
        div
          |
          |
          svg.circle
            include ../svg/circle.pug
          |
          |
    div(data-enllax-ratio="0" data-enllax-type="foreground")
      .parallax-figure
        div.square-dot
          |
          |
          svg.square
            include ../svg/square.pug
          |
          |
          img.dot(src=assets+"/images/mobile/dot.svg" alt="dot")/
          |
          |
