#slide-menu.sidenav.mobile-nav
  .menu
    ul.collection
      each item, index in menu.mobile
        li.collection-item(style="animation-duration: "+(Number(index + 1)) * 0.25 + 's')
          a.sidenav-close.waves-effect.menu-list(href='#' + item.name) #{$t.mobile_landing['header_'+item.name]}
      li.collection-item(style="animation-duration: "+menu.mobile.length * 0.25 + 's')
          a.waves-effect.menu-list(href=link.mobile.contact) #{$t.mobile_landing.header_contact}